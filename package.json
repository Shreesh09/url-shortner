{
  "name": "url-shortner",
  "version": "1.0.0",
  "description": "shortens the given url",
  "scripts": {
    "client": "cd client && npm start",
    "server": "cd server && npm start",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\""
  },
  "devDependencies": {
    "concurrently": "^7.0.0"
  }
}
